        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/ListOfItem"
        android:layout_width="256dp"
        android:layout_height="425dp"
        android:layout_marginStart="79dp"
        android:layout_marginTop="122dp"
